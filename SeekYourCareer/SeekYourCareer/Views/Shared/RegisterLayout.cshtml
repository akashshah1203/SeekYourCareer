<!DOCTYPE html>
<html lang=''>
<head>
   <meta charset='utf-8'>
   <meta http-equiv="X-UA-Compatible" content="IE=edge">
   <meta name="viewport" content="width=device-width, initial-scale=1">
   <link rel="stylesheet" href="~/Content/styles.css">
   <link rel="stylesheet" href="~/Content/Site.css"> 
   <script src="http://code.jquery.com/jquery-latest.min.js" type="text/javascript"></script>
   <script src="~/Content/script.js"></script>
   <title>@ViewBag.Title</title>
    @RenderSection("head", false)
</head>
<body style="background-color:#2980b9">
    <div id="Header" text-align:"right" style="background-color:#000000; width: 100%; top:0px; height: 169px;">
        <img src="~/Content/images/Header1_3.jpg" style="height:auto;" alt="Icon1">
    </div>
        
    <div id="middle" style="width: 100%; position:fixed; top:180px;">
            <div style="position:fixed; left:0px; width:1900px; height: 740px;  float:left; border:1px solid #a1a1a1; padding:10px 10px; border-radius:25px; overflow-y:scroll">
            @RenderBody()
        </div>
    </div>
    <div id="footer" style="position:fixed; height:50px; bottom:0px; left:0px; right:0px; margin-bottom:0px; vertical-align:bottom">
        &#169 SeekYourCareer.com 2015
    </div>
</body>
</html>
